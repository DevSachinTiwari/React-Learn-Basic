class Human{
  constructor(){
    this.gender = 'male'
  }
  printHuman(){
    console.log(this.gender);
  }
}

class Person extends Human{
  constructor(){
    super();
    this.name = 'sachin tiwari';
    this.age = 30;
    //this.gender = 'female'
  }
  printmyName(){
    console.log(this.name);
  }
}
const Details = new Person();
Details.printmyName();
Details.printHuman();


// ES7  how to declare class

class Human{
  gender = 'male';
  printGender = () => {
    console.log(this.gender);
  }
}
const humanPrint = new Human();
humanPrint.printGender();
